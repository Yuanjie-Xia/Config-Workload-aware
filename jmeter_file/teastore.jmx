      <JSR223Listener guiclass="TestBeanGUI" testclass="JSR223Listener" testname="JSR223 Listener">
        <stringProp name="scriptLanguage">groovy</stringProp>
        <stringProp name="parameters"></stringProp>
        <stringProp name="filename"></stringProp>
        <stringProp name="cacheKey">true</stringProp>
        <stringProp name="script">/*
  JSR223 Listener (Groovy) — 3-minute throughput watchdog + cumulative error stop
  Counts ONLY real traffic samplers; explicitly ignores JSR223 Sampler.

  Properties (set via -J or user.properties):
    expected_throughput_per_min   target samples per minute (e.g., 300)
    error_hard_limit              cumulative error hard limit (default 5)
    throughput_tolerance          optional decimal grace (e.g., 0.05); default 0.0

  Usage example (CLI):
    jmeter -n -t test.jmx \
      -Jexpected_throughput_per_min=300 \
      -Jerror_hard_limit=5 \
      -Jthroughput_tolerance=0.05
*/

import groovy.transform.Field
import java.util.concurrent.ConcurrentLinkedQueue
import java.util.concurrent.TimeUnit
import java.util.concurrent.atomic.AtomicInteger
import java.util.concurrent.atomic.AtomicLong
import org.apache.jmeter.engine.StandardJMeterEngine
import org.apache.jmeter.threads.JMeterContextService

// ---- Config ----
@Field final long WINDOW_MS       = TimeUnit.MINUTES.toMillis(3)  // rolling window
@Field final long CHECK_PERIOD_MS = TimeUnit.MINUTES.toMillis(3)  // evaluate cadence
@Field final String IGNORE_CLASS  = &apos;org.apache.jmeter.protocol.java.sampler.JSR223Sampler&apos;

// ---- State (persists across samples) ----
@Field final ConcurrentLinkedQueue&lt;Long&gt; allSampleTs = (allSampleTs ?: new ConcurrentLinkedQueue&lt;Long&gt;())
@Field final ConcurrentLinkedQueue&lt;Long&gt; errorTs     = (errorTs     ?: new ConcurrentLinkedQueue&lt;Long&gt;())
@Field final AtomicInteger cumulativeErrors          = (cumulativeErrors ?: new AtomicInteger(0))
@Field final AtomicLong lastCheckTs                  = (lastCheckTs ?: new AtomicLong(0L))

// ---- Helpers ----
def _propD(String k, double d=Double.NaN) {
    try {
        def s = (props.get(k) as String)
        return (s &amp;&amp; s.trim()) ? (s as double) : d
    } catch (ignored) { d }
}
def _propI(String k, int d=5) {
    try {
        def s = (props.get(k) as String)
        return (s &amp;&amp; s.trim()) ? (s as int) : d
    } catch (ignored) { d }
}

// Read once per invocation
final double targetPerMin = _propD(&apos;expected_throughput_per_min&apos;)
final double tolerance    = Math.max(0d, _propD(&apos;throughput_tolerance&apos;, 0.0d))
final int hardLimit       = _propI(&apos;error_hard_limit&apos;, 5)

// ---- Per-sample bookkeeping (ignore JSR223 Sampler) ----
final long now = System.currentTimeMillis()
def sr = binding.hasVariable(&apos;sampleResult&apos;) ? sampleResult : (binding.hasVariable(&apos;prev&apos;) ? prev : null)
def samplerObj = ctx?.getCurrentSampler()
def samplerClass = samplerObj?.getClass()?.getName()

// Skip any JSR223 Sampler entirely
if (samplerClass == IGNORE_CLASS) {
    return
}

// Count only real traffic samplers
boolean ok = (sr != null) ? sr.isSuccessful() : true
allSampleTs.add(now)
if (!ok) {
    errorTs.add(now)
    cumulativeErrors.incrementAndGet()
}

// Trim outside rolling window
long cutoff = now - WINDOW_MS
while ((allSampleTs.peek() ?: Long.MAX_VALUE) &lt; cutoff) { allSampleTs.poll() }
while ((errorTs.peek()     ?: Long.MAX_VALUE) &lt; cutoff) { errorTs.poll() }

// ---- Immediate hard stop on cumulative errors ----
if (cumulativeErrors.get() &gt; hardLimit) {
    log.info(&quot;Stopping test: cumulative error count ${cumulativeErrors.get()} &gt; hard limit ${hardLimit}&quot;)
    try {
        StandardJMeterEngine.stopEngineNow()
    } catch (t) {
        log.error(&apos;Fallback: stopping thread group due to error.&apos;, t)
        JMeterContextService.getContext().getThreadGroup().stop()
    }
    return
}

// ---- Throttled throughput check every 3 minutes ----
long prevCheck = lastCheckTs.get()
if (prevCheck == 0L) { lastCheckTs.compareAndSet(0L, now); return }
if ((now - prevCheck) &lt; CHECK_PERIOD_MS) { return }
lastCheckTs.set(now)

// Compute real throughput over last 3 minutes
int samplesInWindow = allSampleTs.size()
double realPerMin   = samplesInWindow / 3.0d

if (Double.isNaN(targetPerMin) || targetPerMin &lt;= 0d) {
    log.warn(&quot;expected_throughput_per_min missing/invalid; skipping throughput check this interval.&quot;)
    return
}
double threshold = targetPerMin * (1.0d - tolerance)
int errorsInWindow = errorTs.size()

log.info(String.format(
    &apos;[THROUGHPUT CHECK] window=3min samples=%d real=%.2f/min target=%.2f/min tol=%.2f threshold=%.2f/min errors{window=%d,cumulative=%d}&apos;,
    samplesInWindow, realPerMin, targetPerMin, tolerance, threshold, errorsInWindow, cumulativeErrors.get()
))

if (realPerMin &lt; threshold) {
    log.error(String.format(&apos;Real throughput %.2f/min &lt; threshold %.2f/min — stopping the test.&apos;, realPerMin, threshold))
    try {
        StandardJMeterEngine.stopEngineNow()
    } catch (t) {
        log.error(&apos;Fallback: stopping thread group.&apos;, t)
        JMeterContextService.getContext().getThreadGroup().stop()
    }
}
</stringProp>
      </JSR223Listener>
